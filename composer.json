{
	"name": "orchestra/foundation",
	"description": "Orchestra Platform Foundation Component",
	"keywords": ["orchestra-platform", "laravel"],
	"license": "MIT",
	"authors": [
		{
			"name": "Mior Muhammad Zaki",
			"email": "crynobone@gmail.com"
		}
	],
	"autoload": {
		"psr-0": { 
			"Orchestra\\Foundation" : "src/"
		},
		"classmap" : [
			"src/controllers",
			"src/models"
		],
		"files": [
			"src/helpers.php"
		]
	},
	"require": {
		"php": ">=5.3.3",
		"jasonlewis/basset": "dev-develop",
		"illuminate/foundation": "4.0.*@dev",
		"orchestra/auth": "2.0.*",
		"orchestra/extension": "2.0.*",
		"orchestra/facile": "2.0.*",
		"orchestra/html": "2.0.*",
		"orchestra/memory": "2.0.*",
		"orchestra/support": "2.0.*",
		"orchestra/widget": "2.0.*"
	},
	"require-dev": {
		"mockery/mockery": "0.8.0"
	},
	"extra": {
		"branch-alias": {
			"dev-master": "2.0.x-dev"
		}
	},
	"minimum-stability": "dev"
}
